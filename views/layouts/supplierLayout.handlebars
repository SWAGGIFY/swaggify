      <!-- modal add product-->
    <div class="col-md-6 col-xl-9 grid-margin stretch-card">
    
        <div class="card-body">
        <div class="modal fade" id="AddProduct" tabindex="-1" role="dialog" aria-labelledby="ModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                <h5 class="modal-title" id="ModalLabel">Add New Product</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                </div>
                <form action="/supplier/supplier-add-product" method="POST">
                <div class="modal-body">
                    <div class="form-group">
                    <label for="recipient-name" class="col-form-label">Product Id</label>
                    <input type="number" name="productid" class="form-control" aria-describedby="emailHelp" required>
                    </div>
                    <div class="form-group">
                    <label for="message-text" class="col-form-label">Product name</label>
                    <input type="text" name="productname" class="form-control"  aria-describedby="emailHelp" required>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product Price</label>
                    <input type="text" name="productprice" class="form-control" aria-describedby="emailHelp" required>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product Description</label>
                    <textarea class="form-control" name="productdescription" id="message-text" required></textarea>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product categories</label>
                    <select class="form-control artist" id="category" name="category">
                    </select>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product color</label>
                    <input type="text" name="productcolor" class="form-control" aria-describedby="emailHelp" required>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product size</label>
                    <input type="text" name="productsize" class="form-control" aria-describedby="emailHelp" required>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product Quantity</label>
                    <input type="text" name="productquatity" class="form-control"  aria-describedby="emailHelp" required>
                </div>
                <div class="form-group">
                    <label for="message-text" class="col-form-label">Product Tag</label>
                    <input type="text" name="producttag" class="form-control" aria-describedby="emailHelp" required>
                </div>
                
                </div>
                <div class="modal-footer">
                <button type="submit" class="btn btn-gradient-success">Submit Product</button>
                <button type="button" class="btn btn-light" data-dismiss="modal">Close</button>
                </div>
                </form>
            </div>
            </div>
        </div>
        </div>
    </div>
                    <!-- end of modal product -->

