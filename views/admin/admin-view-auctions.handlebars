<div class="content-wrapper">
          <div class="card">
            <div class="card-body">
              <h4 class="card-title">Data table</h4>
              <div class="row">
                <div class="col-12">
                  <div class="table-responsive">
                    <table id="order-listing" class="table table-borderless">
                      <thead>
                        <tr>
                            <th>User Information</th>
                            <th>Action</th>
                        </tr>
                      </thead>
                      <tbody>
                          <% for (var auction of auctions){ %>
                        <tr data-id="user._id" data-type="user">
                            <td>
                                  <div class="col-md-10 grid-margin stretch-card">
                                    <div class="card">
                                      <div class="card-body">
                                        <div class="d-sm-flex row flex-wrap text-center text-sm-left align-items-center">
                                          <div class="col-md-4">
                                            <img src="/Dashboard/images/faces/face11.jpg" style="width:130px;height:130px; border-radius:56px !important" class="img-lg rounded" alt="profile image"/>
                                          </div>
                                          <div class="col-md-8">
                                            <span class="text-success outline-primary font-weight-bold" style=" top:8px; right:16px; position:absolute;"> </span>
                                            <h5 class="mb-0"><%= auction.name %></h5><hr>
                                            <p class="text-muted mb-1"><%= auction.description %></p>
                                            <p class="mb-0 text-success font-weight-bold">Designer</p>
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                             </td>
                            <td class="action-link">
                                <a class="edit" href="#" title="Edit" data-toggle="modal" data-target="#editDetailModal"><i class="ti-write menu-icon" style="font-size: 1.1rem;line-height: 1;margin-right: 0.6rem;"></i></a>
                                <a class="delete" href="#" title="Delete" data-toggle="modal" data-target="#deleteDetailModal"><i class="ti-trash menu-icon" style="font-size: 1.1rem;line-height: 1;margin-right: 0.6rem;"></i></a>
                            </td>
                        </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- content-wrapper ends -->